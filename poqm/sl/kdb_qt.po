msgstr "Ni bilo mogoče odpreti projektne datoteke \"%1\"."

#: KDbConnection.cpp:511
#, qt-format
msgctxt "KDbConnection|"
msgid "Could not connect to \"%1\" database server."
msgstr "Ni se bilo mogoče povezati do strežnika podatkovne baze \"%1\"."

