#: SqliteAdmin.cpp:43
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not compact database \"%1\"."
msgstr "Ne eblis kompakti datumbazon \"%1\"."

#: SqliteConnection.cpp:49
msgctxt "SqliteConnection|"
msgid "Extra paths for SQLite plugins"
msgstr "Kromaj vojoj por SQLite-kromaĵoj"

#: SqliteConnection.cpp:273
#, qt-format
msgctxt "SqliteConnection|"
msgid ""
"Could not delete file \"%1\". Check the file's permissions and whether it is "
"already opened and locked by another application."
msgstr ""
"Ne eblis forigi dosieron \"%1\". Kontrolu la permesojn de la dosiero kaj ĉu "
"ĝi jam estas malfermita kaj ŝlosita de alia aplikaĵo."

#: SqliteConnection.cpp:375
#, qt-format
msgctxt "SqliteConnection|"
msgid "Could not load SQLite plugin \"%1\"."
msgstr "Ne eblis ŝargi SQLite-aldonaĵon \"%1\"."

#: SqliteConnection.cpp:386
#, qt-format
msgctxt "SqliteConnection|"
msgid "Could not find SQLite plugin file \"%1\"."
msgstr "Ne eblis trovi la kromprogramon de SQLite \"%1\"."

#: SqliteConnection.cpp:401
#, qt-format
msgctxt "SqliteConnection|"
msgid "Could not load SQLite extension \"%1\"."
msgstr "Ne eblis ŝargi la etendon de SQLite \"%1\"."

#: SqliteVacuum.cpp:97
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not find tool \"%1\"."
msgstr "Ne eblis trovi ilon \"%1\"."

#: SqliteVacuum.cpp:105
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not find application \"%1\"."
msgstr "Ne eblis trovi aplikaĵon \"%1\"."

#: SqliteVacuum.cpp:113
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not read file \"%1\"."
msgstr "Ne eblis legi dosieron \"%1\"."

#: SqliteVacuum.cpp:168
msgctxt "SqliteVacuum|"
msgid "Compacting database"
msgstr "Kompakta datumbazo"

#: SqliteVacuum.cpp:170
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Compacting database \"%1\"..."
msgstr "Kompaktante datumbazon \"%1\"..."

#: SqliteVacuum.cpp:259
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not rename file \"%1\" to \"%2\"."
msgstr "Ne eblis renomi dosieron \"%1\" al \"%2\"."

#: SqliteVacuum.cpp:267
#, qt-format
msgctxt "SqliteVacuum|"
msgid ""
"The database has been compacted. Current size decreased by %1% to %2 MB."
msgstr ""
"La datumbazo estis kompaktita. Nuna grandeco malpliiĝis je %1% al %2 MB."
