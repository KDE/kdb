#, qt-format
msgctxt "KDbVariableExpressionData|"
msgid "Table \"%1\" has no \"%2\" field."
msgstr "Tabelis \"%1\" ei ole välja \"%2\"."

#: expression/KDbVariableExpression.cpp:231
#, qt-format
msgctxt "KDbVariableExpressionData|"
msgid "Ambiguous \"%1.%2\" expression"
