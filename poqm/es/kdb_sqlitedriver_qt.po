#: SqliteAdmin.cpp:43
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not compact database \"%1\"."
msgstr "No se ha podido compactar la base de datos «%1»."

#: SqliteConnection.cpp:49
msgctxt "SqliteConnection|"
msgid "Extra paths for SQLite plugins"
msgstr "Rutas adicionales para complementos de SQLite"

#: SqliteConnection.cpp:273
#, qt-format
msgctxt "SqliteConnection|"
msgid ""
"Could not delete file \"%1\". Check the file's permissions and whether it is "
"already opened and locked by another application."
msgstr ""
"No se ha podido borrar el archivo «%1». Compruebe los permisos del archivo y "
"si ya está abierto y bloqueado por otra aplicación."

#: SqliteConnection.cpp:375
#, qt-format
msgctxt "SqliteConnection|"
msgid "Could not load SQLite plugin \"%1\"."
msgstr "No se ha podido cargar el complemento de SQLite «%1»."

#: SqliteConnection.cpp:386
#, qt-format
msgctxt "SqliteConnection|"
msgid "Could not find SQLite plugin file \"%1\"."
msgstr "No se ha podido encontrar el archivo de complemento de SQLite «%1»."

#: SqliteConnection.cpp:401
#, qt-format
msgctxt "SqliteConnection|"
msgid "Could not load SQLite extension \"%1\"."
msgstr "No se ha podido cargar la extensión de SQLite «%1»."

#: SqliteVacuum.cpp:97
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not find tool \"%1\"."
msgstr "No se ha podido encontrar la herramienta «%1»."

#: SqliteVacuum.cpp:105
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not find application \"%1\"."
msgstr "No se ha podido encontrar la aplicación «%1»."

#: SqliteVacuum.cpp:113
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not read file \"%1\"."
msgstr "No se ha podido leer el archivo «%1»."

#: SqliteVacuum.cpp:168
msgctxt "SqliteVacuum|"
msgid "Compacting database"
msgstr "Compactando base de datos"

#: SqliteVacuum.cpp:170
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Compacting database \"%1\"..."
msgstr "Compactando base de datos «%1»..."

#: SqliteVacuum.cpp:259
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not rename file \"%1\" to \"%2\"."
msgstr "No se ha podido cambiar el nombre del archivo «%1» a «%2»."

#: SqliteVacuum.cpp:267
#, qt-format
msgctxt "SqliteVacuum|"
msgid ""
"The database has been compacted. Current size decreased by %1% to %2 MB."
msgstr ""
"Se ha compactado la base de datos. Su tamaño actual ha disminuido un %1% "
"hasta %2 MB."

