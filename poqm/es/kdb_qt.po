msgid "Testing connection to \"%1\" database server..."
msgstr "Probando la conexión con el servidor de bases de datos «%1»..."

#: KDb.cpp:227
#, qt-format
msgctxt "ConnectionTestDialog|"
msgid "Test connection to \"%1\" database server failed."
