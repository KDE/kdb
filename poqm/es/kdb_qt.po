msgstr "Ha ocurrido un error al crear la base de datos «%1» en el servidor."

#: KDbConnection.cpp:743
#, qt-format
msgctxt "KDbConnection|"
msgid "Database \"%1\" has been created but could not be opened."
msgstr "La base de datos «%1» se ha creado, pero no se ha podido abrir."

#: KDbConnection.cpp:819
#, qt-format
msgctxt "KDbConnection|"
msgid "Opening database \"%1\" failed."
msgstr "La apertura de la base de datos «%1» ha fallado."

#: KDbConnection.cpp:904
msgctxt "KDbConnection|"
