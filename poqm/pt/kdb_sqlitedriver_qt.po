#: SqliteAdmin.cpp:43
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not compact database \"%1\"."
msgstr "Não foi possível compactar a base de dados \"%1\"."

#: SqliteConnection.cpp:49
msgctxt "SqliteConnection|"
msgid "Extra paths for SQLite plugins"
msgstr "Localizações extra dos 'plugins' do SQLite"

#: SqliteConnection.cpp:273
#, qt-format
msgctxt "SqliteConnection|"
msgid ""
"Could not delete file \"%1\". Check the file's permissions and whether it is "
"already opened and locked by another application."
msgstr ""
"Não foi possível remover o ficheiro \"%1\". Verifique as permissões do "
"ficheiro e se já está aberto e bloqueado por outra aplicação."

#: SqliteConnection.cpp:375
#, qt-format
msgctxt "SqliteConnection|"
msgid "Could not load SQLite plugin \"%1\"."
msgstr "Não foi possível carregar a extensão do SQLite \"%1\"."

#: SqliteConnection.cpp:386
#, qt-format
msgctxt "SqliteConnection|"
msgid "Could not find SQLite plugin file \"%1\"."
msgstr "Não foi possível encontrar o ficheiro da extensão do SQLite \"%1\"."

#: SqliteConnection.cpp:401
#, qt-format
msgctxt "SqliteConnection|"
msgid "Could not load SQLite extension \"%1\"."
msgstr "Não foi possível carregar a extensão do SQLite \"%1\"."

#: SqliteVacuum.cpp:97
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not find tool \"%1\"."
msgstr "Não foi possível encontrar a ferramenta \"%1\"."

#: SqliteVacuum.cpp:105
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not find application \"%1\"."
msgstr "Não foi possível encontrar a aplicação \"%1\"."

#: SqliteVacuum.cpp:113
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not read file \"%1\"."
msgstr "Não foi possível ler o ficheiro \"%1\"."

#: SqliteVacuum.cpp:168
msgctxt "SqliteVacuum|"
msgid "Compacting database"
msgstr "A compactar a base de dados"

#: SqliteVacuum.cpp:170
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Compacting database \"%1\"..."
msgstr "A compactar a base de dados \"%1\"..."

#: SqliteVacuum.cpp:259
#, qt-format
msgctxt "SqliteVacuum|"
msgid "Could not rename file \"%1\" to \"%2\"."
msgstr "Não foi possível mudar o nome do ficheiro \"%1\" para \"%2\"."

#: SqliteVacuum.cpp:267
#, qt-format
msgctxt "SqliteVacuum|"
msgid ""
"The database has been compacted. Current size decreased by %1% to %2 MB."
msgstr ""
"A base de dados foi compactada. O tamanho actual reduziu-se em %1 para %2 MB."
